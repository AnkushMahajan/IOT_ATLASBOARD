        {"start":1415, "end": 1615}
      ]
    },

    "clockHumidity": {
      "content": "Humidity Reading",
      "interval": 1,
      "stfuOnly": false,
      "stfuHours":[
        {"start":945, "end": 1215},
        {"start":1415, "end": 1615}
      ]
    },

    "clockLight": {
      "content": "Light Reading",
      "interval": 0,
      "stfuOnly": false,
      "stfuHours":[
        {"start":945, "end": 1215},
        {"start":1415, "end": 1615}
      ]
    }
  }
