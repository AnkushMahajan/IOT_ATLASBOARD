{
  "title": "My first dashboard",
  "titleVisible": false,

  "description": "My first dashboard description goes here",

  "layout": {
    "customJS" : ["jquery.peity.js", "md5.js"],
    "gridSize" : { "columns" : 6, "rows" : 4 },
